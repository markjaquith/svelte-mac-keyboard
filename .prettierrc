{
	"useTabs": true,
	"singleQuote": true,
	"semi": false,
	"trailingComma": "es5",
	"printWidth": 100,
	"plugins": [
		"prettier-plugin-svelte"
	],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		},
		{
			"files": "package.json",
			"options": {
				"printWidth": 100,
				"tabWidth": 2,
				"trailingComma": "none",
				"useTabs": false
			}
		}
	]
}